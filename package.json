{
    "name": "@tractorzoom/virtualized-select",
    "version": "1.0.33",
    "description": "React virtualized select element using react-window",
    "main": "lib/index.js",
    "scripts": {
        "build": "./node_modules/.bin/babel src --out-dir lib --copy-files",
        "build-storybook": "build-storybook -s public",
        "commit": "git-cz",
        "deploy-storybook": "sh deploy-gh-pages",
        "pretty-quick": "pretty-quick",
        "storybook": "start-storybook",
        "test": "jest"
    },
    "peerDependencies": {
        "prop-types": "^15.7.2",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-select": "^3.0.8"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TractorZoom/virtualized-select.git"
    },
    "keywords": [
        "react-window",
        "virtualized",
        "select"
    ],
    "author": "Tractor Zoom",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/TractorZoom/virtualized-select/issues"
    },
    "homepage": "https://github.com/TractorZoom/virtualized-select#readme",
    "devDependencies": {
        "@babel/cli": "7.8.4",
        "@babel/core": "7.9.6",
        "@babel/preset-env": "7.9.6",
        "@babel/preset-react": "7.9.4",
        "@commitlint/cli": "8.3.5",
        "@commitlint/config-conventional": "8.3.4",
        "@semantic-release/changelog": "5.0.1",
        "@semantic-release/git": "9.0.0",
        "@semantic-release/release-notes-generator": "9.0.1",
        "@storybook/addon-a11y": "5.3.19",
        "@storybook/addon-actions": "5.3.19",
        "@storybook/addon-console": "1.2.1",
        "@storybook/addon-notes": "5.3.19",
        "@storybook/addon-storysource": "5.3.19",
        "@storybook/react": "5.3.19",
        "babel-jest": "25.5.1",
        "babel-loader": "8.1.0",
        "chance": "1.1.4",
        "commitizen": "4.1.2",
        "cz-conventional-changelog": "3.1.0",
        "husky": "4.2.5",
        "jest": "25.5.4",
        "prettier": "2.0.5",
        "pretty-quick": "2.0.1",
        "prop-types": "15.7.2",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-select": "3.1.0",
        "react-test-renderer": "16.13.1",
        "semantic-release": "17.0.7",
        "storybook-host": "5.1.0"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "pretty-quick --staged"
        }
    },
    "dependencies": {
        "classnames": "^2.2.6",
        "react-window": "^1.8.5"
    }
}
