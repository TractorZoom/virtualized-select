{
    "name": "@tractorzoom/virtualized-select",
    "version": "1.0.8",
    "description": "React virtualized select element using react-window",
    "main": "lib/index.js",
    "scripts": {
        "build": "./node_modules/.bin/babel src --out-dir lib --copy-files",
        "build-storybook": "build-storybook -s public",
        "commit": "git-cz",
        "deploy-storybook": "sh deploy-gh-pages",
        "pretty-quick": "pretty-quick",
        "storybook": "start-storybook",
        "test": "jest"
    },
    "peerDependencies": {
        "prop-types": "^15.7.2",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-select": "^3.0.8"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TractorZoom/virtualized-select.git"
    },
    "keywords": [
        "react-window",
        "virtualized",
        "select"
    ],
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/"
    },
    "author": "Tractor Zoom",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/TractorZoom/virtualized-select/issues"
    },
    "homepage": "https://github.com/TractorZoom/virtualized-select#readme",
    "devDependencies": {
        "@babel/cli": "7.7.4",
        "@babel/core": "7.7.4",
        "@babel/preset-env": "7.7.4",
        "@babel/preset-react": "7.7.4",
        "@commitlint/cli": "8.2.0",
        "@commitlint/config-conventional": "8.2.0",
        "@semantic-release/changelog": "3.0.6",
        "@semantic-release/git": "7.0.18",
        "@semantic-release/release-notes-generator": "7.3.5",
        "@storybook/addon-a11y": "5.2.6",
        "@storybook/addon-actions": "5.2.6",
        "@storybook/addon-console": "1.2.1",
        "@storybook/addon-notes": "5.2.6",
        "@storybook/addon-storysource": "5.2.6",
        "@storybook/react": "5.2.6",
        "babel-jest": "24.9.0",
        "babel-loader": "8.0.6",
        "chance": "1.1.3",
        "commitizen": "4.0.3",
        "cz-conventional-changelog": "3.0.2",
        "husky": "3.1.0",
        "jest": "24.9.0",
        "prettier": "1.19.1",
        "pretty-quick": "2.0.1",
        "prop-types": "15.7.2",
        "react": "16.12.0",
        "react-dom": "16.12.0",
        "react-select": "3.0.8",
        "react-test-renderer": "16.12.0",
        "semantic-release": "15.13.31",
        "storybook-host": "5.1.0"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "pretty-quick --staged"
        }
    },
    "dependencies": {
        "classnames": "^2.2.6",
        "react-virtualized-auto-sizer": "^1.0.2",
        "react-window": "^1.8.5"
    }
}
